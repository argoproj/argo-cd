{
    "Vendor": true,
    "DisableAll": true,
    "Deadline": "8m",
    "Enable": [
        "vet",
        "gofmt",
        "goimports",
        "deadcode",
        "errcheck",
        "varcheck",
        "structcheck",
        "ineffassign",
        "unconvert",
        "misspell"
    ],
    "Linters": {
        "goimports": {"Command": "goimports -l --local github.com/argoproj/argo-cd"}
    },
    "Skip": [
        "pkg/client",
        "vendor/",
        ".pb.go"
    ],
    "Exclude": [
        "pkg/client",
        "vendor/",
        ".pb.go",
        ".*warning.*fmt.Fprint"
    ]
}
